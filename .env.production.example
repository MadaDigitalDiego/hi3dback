# Configuration pour la production
APP_NAME="Hi3D Backend"
APP_ENV=production
APP_KEY=base64:your_app_key_here
APP_DEBUG=false
APP_URL=https://dev-backend.hi-3d.com

# Configuration CORS et Sanctum
FRONTEND_URL=https://your-frontend-domain.com
SANCTUM_STATEFUL_DOMAINS=your-frontend-domain.com,dev-backend.hi-3d.com

# Base de donn√©es
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=hi3d_production
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password

# Configuration des sessions
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_DOMAIN=.hi-3d.com
SESSION_SECURE_COOKIE=true
SESSION_SAME_SITE=none

# Configuration des cookies
COOKIE_DOMAIN=.hi-3d.com

# Autres configurations...
LOG_CHANNEL=stack
LOG_LEVEL=error
CACHE_DRIVER=redis
QUEUE_CONNECTION=redis
